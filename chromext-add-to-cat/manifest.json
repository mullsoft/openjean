{
  "manifest_version": 2,

  "name": "Add URL to Category",
  "description": "This extension posts the current tab url into a category",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://www.mullsoft.co.uk/",
    "http://shrewsburylabour.org.uk/"
  ]
}
